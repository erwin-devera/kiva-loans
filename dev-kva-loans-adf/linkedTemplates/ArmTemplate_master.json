{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-kva-loans-adf"
		},
		"ls_ablob_kivaloans_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_kivaloans'"
		},
		"ls_dl_kivaloans_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_dl_kivaloans'"
		},
		"ls_dl_kivaloans_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devkivaloansdl.dfs.core.windows.net/"
		},
		"ls_kivaloans_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dev-kiva-loans-kv.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dev-kva-loans-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_kivaloans_connectionString": {
						"value": "[parameters('ls_ablob_kivaloans_connectionString')]"
					},
					"ls_dl_kivaloans_accountKey": {
						"value": "[parameters('ls_dl_kivaloans_accountKey')]"
					},
					"ls_dl_kivaloans_properties_typeProperties_url": {
						"value": "[parameters('ls_dl_kivaloans_properties_typeProperties_url')]"
					},
					"ls_kivaloans_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kivaloans_kv_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}